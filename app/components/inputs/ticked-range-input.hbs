<datalist id="{{this.elementId}}InputRangeTicks">
  {{#each this.tickList as |tick|}}
    <option>{{tick}}</option>
  {{/each}}
</datalist>

<div class="range-color" style="background-color: {{this.backgroundColor}}">
  <Inputs::RangeInput @config={{this.config}} id="{{this.elementId}}InputRange" list="{{this.elementId}}InputRangeTicks" />
</div>
